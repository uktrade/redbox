{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "docker-compose up deps",
            "type": "shell",
            "command": "docker compose up -d unstructured db minio opensearch opensearch-dashboards",
            "options": {
                "cwd": "${workspaceFolder}",
                "env": {
                    "DOCKER_DEFAULT_PLATFORM": "linux/amd64"
                }
